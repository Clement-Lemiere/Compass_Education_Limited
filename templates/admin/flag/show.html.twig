{% extends 'base.html.twig' %}

{% block title %}Flag{% endblock %}

{% block body %}
    <h1>Flag</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ flag.id }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ flag.country }}</td>
            </tr>
            <tr>
                <th>IsoCode</th>
                <td>{{ flag.isoCode }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ flag.image }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_flag_index') }}">back to list</a>

    <a href="{{ path('app_admin_flag_edit', {'id': flag.id}) }}">edit</a>

    {{ include('admin/flag/_delete_form.html.twig') }}
{% endblock %}
