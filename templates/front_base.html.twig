<!DOCTYPE html>
<html>
    <head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Fira+Sans&family=Lobster&family=Raleway:wght@300;400;700&display=swap" rel="stylesheet">   
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
    </head>
    {% block header %}
        <div {{ react_component('FrontNav') }}></div> 
    {% endblock %}
    {% block body %}
    {% endblock %}
    {% block footer %}
        <footer {{ react_component('Footer') }}></footer>
    {% endblock %}
    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</html> 